{
  "name": "Real Estate Prices for Chrome",
  "description": "A Chrome extension that lets you view RealEstate.com house prices at the click of the icon.",
  "version": "0.0.5",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
  "permissions": ["storage", "activeTab", "contextMenus", "scripting"],
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  }
}
